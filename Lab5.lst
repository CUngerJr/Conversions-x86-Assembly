 000000BC  E8 00000000 E	 CALL Crlf
 000000C1  B8 00000000		 MOV eax, 0

 000000C6  C3			 RET
 000000C7			 Opt1 ENDP


				 ;************************************************************************************************
 000000C7			 Opt2 PROC
				;
				; Converts user's hexadecimal value to integer value
				; Receivers: EAX receives user's hexadecimal value by the CALL ReadHex and
				;			 pass the parameter back to EAX. 
				; Returns: Then, the CALL WriteDec will return the unsigned Integer value back in EAX.
				;************************************************************************************************

 000000C7  BA 00000104 R	 MOV edx, OFFSET msgValueHex
 000000CC  E8 00000000 E	 CALL WriteString
 000000D1  E8 00000000 E	 CALL ReadHex
 000000D6  E8 00000000 E	 CALL WriteDec
 000000DB  E8 00000000 E	 CALL Crlf
 000000E0  E8 00000000 E	 CALL Crlf
 000000E5  B8 00000000		 MOV eax, 0

 000000EA  C3			 RET
 000000EB			 Opt2 ENDP

				 ;************************************************************************************************
 000000EB			 Opt3 PROC
